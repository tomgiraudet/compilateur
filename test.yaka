PROGRAMME testGen

ENTIER FONCTION max (ENTIER i, ENTIER j)
VAR BOOLEEN b,c;
VAR ENTIER k;
k=i;
SI k > j ALORS RETOURNE k
SINON RETOURNE j
FSI
FFONCTION

ENTIER FONCTION min (ENTIER i, ENTIER j)
SI i < j ALORS RETOURNE i
SINON RETOURNE j
FSI
FFONCTION

BOOLEEN FONCTION egal (ENTIER a, ENTIER b)
VAR ENTIER diffab, diffba;
VAR BOOLEEN boolDeNegro, boolDeMeufTropBonne, boolUltime;

	diffab = a-b;
	diffba = b-a;

	SI diffab = 0 ALORS
		boolDeNegro = VRAI
	FSI;

	SI diffba = 0 ALORS
		boolDeMeufTropBonne = VRAI
	FSI;

	SI boolDeNegro = boolDeMeufTropBonne ALORS
		boolUltime = VRAI
	FSI;

	RETOURNE boolUltime;

FFONCTION

ENTIER FONCTION func (ENTIER a, ENTIER b, ENTIER c, ENTIER d)
VAR ENTIER res1, res2, tmp21, tmp22, RES;
VAR BOOLEEN boolDeNegro;
	res1  = 3*(max(a,b))+min(c,d);

	tmp21 = max(a,b);
	tmp21 = 2*tmp21 + max(a,b);
	tmp22 = min(c,d);
	res2 = tmp21 + tmp22;

	SI egal(res1,res2) ALORS
		RES = 0
	SINON
		RES = -1000
	FSI;

	RETOURNE tmp21+RES;

FFONCTION


PRINCIPAL
VAR ENTIER a, b, c, d, resFunc;
	a = 6;
	b = 9;
	c = 7;
	d = 4;

	ALALIGNE;

	RETOURNE d;

	ECRIRE(a);
	ECRIRE(b);
	ECRIRE(c);
	ECRIRE(d);

	ALALIGNE;

	resFunc = func(a,b,c,d);
	ECRIRE(resFunc);
FPRINCIPAL

FPROGRAMME
